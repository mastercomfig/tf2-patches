"VertexLitGeneric"
{
	"$baseTexture" "models/workshop/weapons/c_models/c_uberneedle/c_uberneedle"
	"$basemapalphaphongmask" "1"
	
	"$phong" "1"
	"$phongexponent" "5"
	"$phongboost" "0.1"	
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$phongfresnelranges"	"[1 2 1]"
	

	// Rim lighting parameters
	"$rimlight" "1"
	"$rimlightexponent" "4"
	"$rimlightboost" ".65"

	
	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}